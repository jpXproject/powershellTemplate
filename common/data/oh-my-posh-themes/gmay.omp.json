{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#577e8d",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "prefix": "  ",
            "style": "full"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "background": "#407742",
          "foreground": "#ffef09",
          "powerline_symbol": "",
          "properties": {
            "prefix": " ",
            "time_format": "2006-01-02 03:04 AM"
          },
          "style": "powerline",
          "type": "time"
        },
        {
          "background": "#fffb38",
          "foreground": "#193549",
          "powerline_symbol": "",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}"
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "prefix": "  "
          },
          "style": "powerline",
          "type": "node"
        },
        {
          "background": "#ffff66",
          "foreground": "#193549",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "#2e9599",
          "background_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"],
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true,
            "template": ""
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#f1184c",
          "properties": {
            "prefix": "",
            "text": ""
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
